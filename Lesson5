def print_info(func):
    def wrapper(*args,**kwargs):
        print('Start')
        result = func(*args,**kwargs)
        print('End')
        return result
    return wrapper

@print_info
def add_num(a, b):
    return a + b

r = add_num(10,20)
print(r)

@print_info
def sub_num(a, b):
    return a - b

r = sub_num(10,20)
print(r)
